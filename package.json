{
  "name": "portafolio",
  "version": "0.1.0",
  "private": true,
  "homepage": "https://rmaafs.com/",
  "dependencies": {
    "@material-ui/core": "^4.11.4",
    "@testing-library/jest-dom": "^5.12.0",
    "@testing-library/react": "^11.2.6",
    "@testing-library/user-event": "^12.8.3",
    "react": "^17.0.2",
    "react-animate-on-scroll": "^2.1.5",
    "react-dom": "^17.0.2",
    "react-fade-in": "^2.0.1",
    "react-loadingg": "^1.7.2",
    "react-scripts": "4.0.3",
    "web-vitals": "^1.1.1"
  },
  "scripts": {
    "test:sintax": "prettier-check src/**/*.jsx",
    "sintax:fix": "prettier-check --write src/**/*.jsx",
    "mytest": "npm run test:sintax",
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "predeploy": "npm run mytest && npm run build",
    "deploy": "npm run mytest && gh-pages -d build"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "check-files-exist": "^1.0.1",
    "gh-pages": "^3.1.0",
    "prettier": "^2.3.1",
    "prettier-check": "^2.0.0"
  }
}
